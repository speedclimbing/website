{
	"name": "speedclimbing-website",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"pages": "wrangler pages dev .svelte-kit/cloudflare",
		"tail": "wrangler pages deployment tail --project-name speedclimbing --environment preview"
	},
	"devDependencies": {
		"@playwright/test": "^1.40.1",
		"@popperjs/core": "^2.11.8",
		"@sveltejs/adapter-auto": "^3.1.0",
		"@sveltejs/kit": "^2.0.6",
		"@types/chart.js": "^2.9.41",
		"@typescript-eslint/eslint-plugin": "^6.17.0",
		"@typescript-eslint/parser": "^6.17.0",
		"autoprefixer": "^10.4.16",
		"classnames": "^2.5.1",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"flowbite": "^2.2.1",
		"flowbite-svelte": "^0.44.21",
		"postcss": "^8.4.33",
		"postcss-each": "^1.1.0",
		"postcss-load-config": "^5.0.2",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.8",
		"svelte-check": "^3.6.2",
		"svelte-heros-v2": "^0.10.12",
		"svelte-preprocess": "^5.1.3",
		"svelte-turnstile": "^0.5.0",
		"tailwindcss": "^3.4.0",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"vite": "^5.0.11",
		"vitest": "^1.1.3",
		"wrangler": "^3.22.3"
	},
	"type": "module",
	"dependencies": {
		"chart.js": "^4.4.1",
		"fullcalendar": "^6.1.10"
	}
}
